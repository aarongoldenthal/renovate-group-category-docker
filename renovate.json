{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", "docker:disableMajor"],
  "packageRules": [
    {
      "matchPackageNames": [
        "registry.gitlab.com/gitlab-ci-utils/docker-puppeteer"
      ],
      "versioning": "regex:^node-(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)-(?<compatibility>.*)$"
    }
  ]
}
